@using Bill2Pay.Model
@*@model SubmissionDetail*@
@model ImportDetail

@{
    ViewBag.Title = "1099 K Details";
}

@{
    SubmissionStatus status = null;

    if (Model.SubmissionSummaryId != null)
    {
        status = ApplicationDbContext.Instence.SubmissionStatus
                        .FirstOrDefault(p => p.AccountNumber.Equals(Model.AccountNo, StringComparison.OrdinalIgnoreCase) && p.PaymentYear == Model.ImportSummary.PaymentYear && p.IsActive == true);
    }
}
<div class="row">
    <div ng-app="tabPanel" ng-controller="tabPanelCtrl as ctrl" ng-click="tabChange($event)">
        <div class="col-md-12">
            <ul class="nav nav-tabs page-tabs">
                <li ng-class="{'active':ctrl.tabSelected=='#copy-a'}"><a href="#copy-a">Copy A</a></li>
                <li ng-class="{'active':ctrl.tabSelected=='#copy-1'}"><a href="#copy-1">Copy 1</a></li>
                <li ng-class="{'active':ctrl.tabSelected=='#copy-b'}"><a href="#copy-b">Copy B</a></li>
                <li ng-class="{'active':ctrl.tabSelected=='#copy-2'}"><a href="#copy-2">Copy 2</a></li>
                <li ng-class="{'active':ctrl.tabSelected=='#copy-c'}"><a href="#copy-c">Copy C</a></li>
            </ul>

        </div>
        <div class="col-md-12">
            <br />
            <div id="copy-a" class="ng-hide" ng-show="ctrl.tabSelected=='#copy-a'">
                @if (status != null && status.Id > 2)
                {
                    using (Html.BeginForm("CopyA", "Print", new { ReturnUrl = ViewBag.ReturnUrl, Id = Model.AccountNo }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                    {
                        @Html.AntiForgeryToken()
                        <button style="float:right;" type="submit" class="btn btn-primary btn-lg " title="Print to PDF">
                            <span class="glyphicon glyphicon-print"></span>
                        </button>
                    }
                }
                <div class="copy-a">
                    <div>
                        <span style="position:absolute; left:226px; top:20px"><b>@((status != null && status.StatusId == 6) ? "x" : "")</b></span>
                        <span style="position:absolute; left:314px; top:20px"><b>@((status != null && (status.StatusId == 3 || status.StatusId == 4 || status.StatusId == 5)) ? "x" : "")</b></span>
                    </div>
                    <div>
                        <span style="position:absolute; left:25px; top:65px"> @Model.Merchant.Payer.Transmitter.CompanyName </span>
                        <span style="position:absolute; left:25px; top:90px"> @Model.Merchant.Payer.Transmitter.CompanyMailingAddress </span>
                        <span style="position:absolute; left:25px; top:110px">@Model.Merchant.Payer.Transmitter.CompanyCity</span>
                        <span style="position:absolute; left:25px; top:130px">@Model.Merchant.Payer.Transmitter.CompanyState</span>
                        <span style="position:absolute; left:25px; top:150px">@Model.Merchant.Payer.Transmitter.CompanyZIP</span>
                    </div>
                    <div>
                        @{
                            if (@Model.FillerIndicatorType == "1")
                            {
                                <span style="position:absolute; left:185px; top:213px"><b>x</b></span>
                            }
                            else if (@Model.FillerIndicatorType == "2")
                            {
                                <span style="position:absolute; left:185px; top:238px"><b>x</b></span>
                            }

                            if (@Model.PaymentIndicatorType == "1")
