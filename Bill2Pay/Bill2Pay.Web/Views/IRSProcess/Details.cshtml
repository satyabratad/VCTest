<script>
    var app = angular.module("tabPanel", []);
    app.controller("tabPanelCtrl", function ($scope) {
        var self = this;
        self.tabSelected = "#copy-a";
        $scope.tabChange = function (e) {
            if (e.target.nodeName === 'A') {
                self.tabSelected = e.target.getAttribute("href");
                e.preventDefault();
            }
        }
    })
</script>
