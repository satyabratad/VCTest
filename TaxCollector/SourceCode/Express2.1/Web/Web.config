      <error statusCode="500" redirect="/Errors/HttpError.aspx?errCode=500" />
      <error statusCode="503" redirect="/Errors/HttpError.aspx?errCode=503" />
    </customErrors>

    <pages clientIDMode="Static" />
    <globalization culture="auto" uiCulture="auto" />

    <authorization>
      <deny verbs="OPTIONS" users="*"/>
