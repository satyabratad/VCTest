    <customErrors mode="RemoteOnly" defaultRedirect="/Error.aspx">
      <error statusCode="400" redirect="/Errors/HttpError.aspx?errCode=400" />
      <error statusCode="401" redirect="/Errors/HttpError.aspx?errCode=401" />
      <error statusCode="402" redirect="/Errors/HttpError.aspx?errCode=402" />
