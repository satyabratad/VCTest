var products = [];
var bill2payProducts = {


    addProducts: function () {
       debugger;
		var selectedProduct=$("#ddlCategories").val();
		if(selectedProduct.toUpperCase()=="TAX BILL")
		{
			var product = {
								"ProductName": selectedProduct,
								"Parcel":  $("#txtLookupAccount1").val(),
								"TaxYear": $("#txtLookupAccount2").val(),
								"OwnerName": $("#txtLookupAccount3").val()
						  };
			
			products.push(product);
			
		}
		else if(selectedProduct.toUpperCase()=="DMV APPLICATION")
		{
			var product = {
								"ProductName": selectedProduct,
								"LicenseNumber":  $("#txtLookupAccount1").val(),
						  };
			
			products.push(product);
			
		}
    },

	submitAccDetails: function () {
		debugger;
		bill2payProducts.addProducts();
       window.location.href = 'file:///D:/Bill2Pay%20Html/Home.html?products=' + JSON.stringify(products)
    }
};